<Window x:Class="ADP2_FLIGHTGEAR.View.DataInvestigation"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ADP2_FLIGHTGEAR.View"
        xmlns:oxy="http://oxyplot.org/wpf"
        mc:Ignorable="d"
        Title="Data Investigation" Height="450" Width="800">
    <Grid>
        <Image Source="../backgrounds/low-angle-shot-airplane-descending-from-clouded-sky_181624-26225.jpg" Stretch="Fill"/>
        <Grid >
            <oxy:PlotView x:Name="BasicGraph" Model ="{Binding PlotBasicGraph}" Margin="10,10,450,200" BorderBrush="DarkBlue" />
        </Grid>

        <Button x:Name="chosenValue" Content="Change value" Margin="459,54,241,327" Click="Change_Value" Background="LightBlue" Foreground="DarkBlue"/>
        <Button Content="Return to the main" HorizontalAlignment="Left" Margin="645,10,0,0" VerticalAlignment="Top" Width="139" Click="Mainwindow_Click" Height="30" Background="Blue" Foreground="White"/>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="433,18,0,0" VerticalAlignment="Top" Width="152"/>
    </Grid>
</Window>
